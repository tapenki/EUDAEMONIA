# pyromancy
msgid "pyromancy_title"
msgstr "[outline_color=#ff0000]Pyromancy[/outline_color]"

msgid "pyromancy_description"
msgstr "Set people on fire yay !!!"

msgid "ignition_title"
msgstr "[outline_color=#ff0000]Ch.1: Ignition[/outline_color]"

msgid "ignition_description"
msgstr "[outline_size=10]Upon dealing damage:[/outline_size] Apply [outline_size=10]+[2xLEVEL][/outline_size] burn."

msgid "firespawning_title"
msgstr "[outline_color=#ff0000]Ch.2: Firespawning[/outline_color]"

msgid "firespawning_description"
msgstr "[outline_size=10]Upon day start:[/outline_size] Summon [outline_size=10][4][/outline_size] flaming skulls. They have [outline_size=10]+[35xLEVEL][/outline_size] health, [outline_size=10]x[LEVEL][/outline_size] damage, and apply [outline_size=10]+[10xLEVEL][/outline_size] burn to nearby foes when killed."

msgid "melt_title"
msgstr "[outline_color=#ff0000]Ch.3: Melt[/outline_color]"

msgid "melt_description"
msgstr "Gain [outline_size=10]+[0.25xLEVELxSTACKS][/outline_size] damage against burning foes."

msgid "scorched_earth_title"
msgstr "[outline_color=#ff0000]Ch.4: Scorched Earth[/outline_color]"

msgid "scorched_earth_description"
msgstr "[outline_size=10]Upon projectile or summon expiration:[/outline_size] It leaves a pool of fire that deals [outline_size=10]x[0.25xLEVEL][/outline_size] damage."

msgid "from_ashes_title"
msgstr "[outline_color=#ff0000]Ch.5: From Ashes[/outline_color]"

msgid "from_ashes_description"
msgstr "[outline_size=10]Once a day, upon your imminent demise:[/outline_size] Escape your fate and recover [outline_size=10]+[10xLEVEL][/outline_size] health per second over [outline_size=10][5][/outline_size] seconds."

msgid "conflagration_title"
msgstr "[outline_color=#ff0000]Mastery: Conflagration[/outline_color]"

msgid "conflagration_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#ff0000]Ignition[/outline_color][br][outline_size=10]Upon a burning creature dying:[/outline_size] Spread its burn to the nearest foe."

msgid "undying_flames_title"
msgstr "[outline_color=#ff0000]Mastery: Undying Flames[/outline_color]"

msgid "undying_flames_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#ff0000]Firespawning[/outline_color][br][outline_size=10]Upon a flaming skull dying:[/outline_size] Reconstitute it."

msgid "fiery_rebirth_title"
msgstr "[outline_color=#ff0000]Mastery: Fiery Rebirth[/outline_color]"

msgid "fiery_rebirth_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#ff0000]From Ashes[/outline_color][br][outline_size=10]Upon [/outline_size][outline_color=#ff0000]From Ashes[/outline_color][outline_size=10] activation:[/outline_size] Apply [outline_size=10]+[10xFROMASHESLEVEL][/outline_size] burn to all enemies then double their burn."


msgid "burn_title"
msgstr "[outline_color=#ff0000]Notes: Burn[/outline_color]"

msgid "burn_description"
msgstr "Deals [outline_size=10][STACKS][/outline_size] damage every second. Expires after dealing damage [outline_size=10][5][/outline_size] times. Reapplying burn resets expiration."

# cryomancy
msgid "cryomancy_title"
msgstr "[outline_color=#0000ff]Cryomancy[/outline_color]"

msgid "cryomancy_description"
msgstr "Set people on ice yay !!!"

msgid "blue_needle_title"
msgstr "[outline_color=#0000ff]Ch.1: Blue Needle[/outline_color]"

msgid "blue_needle_description"
msgstr "Projectiles gain [outline_size=10]+[LEVEL][/outline_size] pierce."

msgid "snowball_title"
msgstr "[outline_color=#0000ff]Ch.2: Snowball[/outline_color]"

msgid "snowball_description"
msgstr "Projectiles gain [outline_size=10]x[0.5xLEVEL][/outline_size] bonus damage and [outline_size=10]x[1-0.5^LEVEL][/outline_size] bonus size per second."

msgid "frostbite_title"
msgstr "[outline_color=#0000ff]Ch.3: Frostbite[/outline_color]"

msgid "frostbite_description"
msgstr "[outline_size=10]Upon dealing damage:[/outline_size] Apply a [outline_size=10]x[0.8][/outline_size] speed chill for [outline_size=10]+[0.75xLEVEL][/outline_size] seconds."

msgid "shatter_title"
msgstr "[outline_color=#0000ff]Ch.4: Shatter[/outline_color]"

msgid "shatter_description"
msgstr "[outline_size=10]Upon a creature dying:[/outline_size] It releases [outline_size=10][3][/outline_size] bullets with [outline_size=10]x[LEVEL][/outline_size] damage. Summoned creatures only release [outline_size=10][1][/outline_size] bullet."

msgid "cryobombs_title"
msgstr "[outline_color=#0000ff]Ch.5: Cryobombs[/outline_color]"

msgid "cryobombs_description"
msgstr "[outline_size=10]Upon day start:[/outline_size] Summon [outline_size=10][2][/outline_size] cryobombs. They have [outline_size=10]x[0.25xLEVEL][/outline_size] damage, store taken damage, and explode dealing [outline_size=10]+[STOREDDAMAGE][/outline_size] damage [outline_size=10][3][/outline_size] seconds after getting hit."

msgid "snowball_ii_title"
msgstr "[outline_color=#0000ff]Mastery: Snowball II[/outline_color]"

msgid "snowball_ii_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#0000ff]Snowball[/outline_color][br]Projectiles gain [outline_size=10]x[0.5xSNOWBALLLEVEL][/outline_size] bonus crit chance and status applied per second."

msgid "snap_freeze_title"
msgstr "[outline_color=#0000ff]Mastery: Snap Freeze[/outline_color]"

msgid "snap_freeze_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#0000ff]Frostbite[/outline_color][br][outline_size=10]Upon dealing damage to an undamaged creature:[/outline_size] Apply a [outline_size=10]x[0][/outline_size] speed freeze for [outline_size=10]+[0.15xFROSTBITELEVEL][/outline_size] seconds."

msgid "shard_blast_title"
msgstr "[outline_color=#0000ff]Polymastery: Shard Blast[/outline_color]"

msgid "shard_blast_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][3][/outline_size] [outline_color=#0000ff]Shatter[/outline_color] and level [outline_size=10][3][/outline_size] [outline_color=#0000ff]Cryobombs[/outline_color][br]Cryobombs realease [outline_size=10][12][/outline_size] [outline_color=#0000ff]Shatter[/outline_color] bullets with [outline_size=10]x[2][/outline_size] damage upon exploding."

# electromancy
msgid "electromancy_title"
msgstr "[outline_color=#a87e00]Electromancy[/outline_color]"

msgid "electromancy_description"
msgstr "i love electricity."

msgid "chain_bolt_title"
msgstr "[outline_color=#a87e00]Ch.1: Chain Bolt[/outline_color]"

msgid "chain_bolt_description"
msgstr "Gain [outline_size=10]+[10xLEVEL]%[/outline_size] crit chance. Critical damage chains to the closest foe up to [outline_size=10][2][/outline_size] times."

msgid "mortal_shock_title"
msgstr "[outline_color=#a87e00]Ch.2: Mortal Shock[/outline_color]"

msgid "mortal_shock_description"
msgstr "Gain [outline_size=10]+[25xLEVEL]%[/outline_size] crit chance against undamaged foes and [outline_size=10]+[5xLEVEL][/outline_size] damage on crit."

msgid "ball_lightning_title"
msgstr "[outline_color=#a87e00]Ch.3: Ball Lightning[/outline_color]"

msgid "ball_lightning_description"
msgstr "[outline_size=10]Upon day start:[/outline_size] Manifest [outline_size=10][2][/outline_size] ball lightnings orbiting around you. They have [outline_size=10]x[LEVEL][/outline_size] damage and [outline_size=10]x[2]%[/outline_size] crit chance."

msgid "electromagnetism_title"
msgstr "[outline_color=#a87e00]Ch.4: Electromagnetism[/outline_color]"

msgid "electromagnetism_description"
msgstr "Projectiles home into enemies within [outline_size=10][50+50xLEVEL]px[/outline_size]."

msgid "thunderstep_title"
msgstr "[outline_color=#a87e00]Ch.5: Thunderstep[/outline_color]"

msgid "thunderstep_description"
msgstr "You gain [outline_size=10]+[50xLEVEL][/outline_size] move speed and [outline_size=10]+[0.2xLEVEL][/outline_size] seconds of immunity when hit."

msgid "shocking_grasp_title"
msgstr "[outline_color=#a87e00]Mastery: Shocking Grasp[/outline_color]"

msgid "shocking_grasp_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#a87e00]Mortal Shock[/outline_color][br][outline_color=#a87e00]Mortal Shock's[/outline_color] crit chance bonus persists for [outline_size=10][2.5][/outline_size] seconds"

msgid "dynamo_title"
msgstr "[outline_color=#a87e00]Mastery: Dynamo[/outline_color]"

msgid "dynamo_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#a87e00]Ball Lightning[/outline_color][br][outline_size=10]Upon day start:[/outline_size] Manifest [outline_size=10]+[2][/outline_size] ball lightnings"

msgid "repulsar_title"
msgstr "[outline_color=#a87e00]Polymastery: Repulsar[/outline_color]"

msgid "repulsar_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][3][/outline_size] [outline_color=#a87e00]Electromagnetism[/outline_color] and level [outline_size=10][3][/outline_size] [outline_color=#a87e00]Thunderstep[/outline_color][br]You divert nearby enemy projectiles."

msgid "crit_title"
msgstr "[outline_color=#a87e00]Notes: Crit[/outline_color]"

msgid "crit_description"
msgstr "When dealing damage, roll for crits. Damage is increased by [outline_size=10]x[1+CRITCOUNT][/outline_size]. Crit chance over [outline_size=10][100]%[/outline_size] is converted into extra crits."

