msgid "fire_title"
msgstr "[outline_color=#ff0000]Fire[/outline_color]"

msgid "fire_description"
msgstr "Set people on fire yay !!!"

msgid "ignition_title"
msgstr "[outline_color=#ff0000]Ability: Ignition[/outline_color]"

msgid "ignition_description"
msgstr "[outline_size=10][33]% upon dealing damage:[/outline_size] Apply [outline_size=10][2xLEVEL][/outline_size] burn."

msgid "firespawning_title"
msgstr "[outline_color=#ff0000]Ability: Firespawning[/outline_color]"

msgid "firespawning_description"
msgstr "[outline_size=10]Upon encounter:[/outline_size] Summon [outline_size=10][4][/outline_size] flaming skulls. They have [outline_size=10][35xLEVEL][/outline_size] health, [outline_size=10]x[0.5xLEVEL][/outline_size] damage, and apply [outline_size=10][10xLEVEL][/outline_size] burn to nearby enemies when killed."

msgid "melt_title"
msgstr "[outline_color=#ff0000]Ability: Melt[/outline_color]"

msgid "melt_description"
msgstr "Gain [outline_size=10]+[0.25xLEVELxSTACKS][/outline_size] damage against burning enemies."

msgid "scorched_earth_title"
msgstr "[outline_color=#ff0000]Ability: Scorched Earth[/outline_color]"

msgid "scorched_earth_description"
msgstr "[outline_size=10]Upon a summon dying or [50]% upon a projectile expiring:[/outline_size] Manifest a pool of fire that deals [outline_size=10]x[0.25xLEVEL][/outline_size] damage."

msgid "from_ashes_title"
msgstr "[outline_color=#ff0000]Ability: From Ashes[/outline_color]"

msgid "from_ashes_description"
msgstr "[outline_size=10]Once per encounter, upon your imminent demise:[/outline_size] Escape your fate and recover [outline_size=10]+[10xLEVEL][/outline_size] health per second over [outline_size=10][5][/outline_size] seconds."

msgid "conflagration_title"
msgstr "[outline_color=#ff0000]Mastery: Conflagration[/outline_color]"

msgid "conflagration_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#ff0000]Ignition[/outline_color][br][outline_size=10]Upon a burning entity dying:[/outline_size] Spread its burn to the nearest enemy."

msgid "undying_flames_title"
msgstr "[outline_color=#ff0000]Mastery: Undying Flames[/outline_color]"

msgid "undying_flames_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#ff0000]Firespawning[/outline_color][br][outline_size=10]Upon a flaming skull dying:[/outline_size] Reconstitute it."

msgid "white_hot_title"
msgstr "[outline_color=#ff0000]Mastery: White Hot[/outline_color]"

msgid "white_hot_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#ff0000]Melt[/outline_color][br]Gain [outline_size=10]+[100]%[/outline_size] crit chance against enemies with [outline_size=10][50][/outline_size] or more burn stacks."

msgid "ring_of_fire_title"
msgstr "[outline_color=#ff0000]Ability: Ring of Fire[/outline_color]"

msgid "ring_of_fire_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#ff0000]Scorched Earth[/outline_color][br][outline_size=10]Upon encounter:[/outline_size] Manifest a ring of fire around you. It has [outline_size=10]x[0.25xLEVEL][/outline_size] damage."

msgid "fiery_rebirth_title"
msgstr "[outline_color=#ff0000]Mastery: Fiery Rebirth[/outline_color]"

msgid "fiery_rebirth_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#ff0000]From Ashes[/outline_color][br][outline_size=10]Upon [/outline_size][outline_color=#ff0000]From Ashes[/outline_color][outline_size=10] activation:[/outline_size] Apply [outline_size=10]+[5xLEVEL][/outline_size] burn to all enemies then double their burn."

msgid "burn_title"
msgstr "[outline_color=#ff0000]Notes: Burn[/outline_color]"

msgid "burn_description"
msgstr "Deals [outline_size=10][STACKS][/outline_size] damage every second.[br]Expires after dealing damage [outline_size=10][5][/outline_size] times.[br]Reapplying burn resets expiration."

# ice
msgid "ice_title"
msgstr "[outline_color=#0000ff]Ice[/outline_color]"

msgid "ice_description"
msgstr "Delay the inevitable.[br]Allow enemies to break."

msgid "blue_needle_title"
msgstr "[outline_color=#0000ff]Parability: Blue Needle[/outline_color]"

msgid "blue_needle_description"
msgstr "Projectiles gain [outline_size=10]+[LEVEL][/outline_size] pierce."

msgid "snowball_title"
msgstr "[outline_color=#0000ff]Ability: Snowball[/outline_color]"

msgid "snowball_description"
msgstr "Projectiles gain [outline_size=10]x[0.25xLEVEL][/outline_size] bonus damage and [outline_size=10]x[1-0.5^LEVEL][/outline_size] bonus size per second."

msgid "frostbite_title"
msgstr "[outline_color=#0000ff]Ability: Frostbite[/outline_color]"

msgid "frostbite_description"
msgstr "[outline_size=10][33]% upon dealing damage:[/outline_size] Apply chill for [outline_size=10][0.75xLEVEL][/outline_size] seconds."

msgid "shatter_title"
msgstr "[outline_color=#0000ff]Ability: Shatter[/outline_color]"

msgid "shatter_description"
msgstr "[outline_size=10]Upon an entity dying:[/outline_size] It releases [outline_size=10][3][/outline_size] projectiles with [outline_size=10]x[0.5xLEVEL][/outline_size] damage. Summoned entities only release [outline_size=10][1][/outline_size] projectile."

msgid "cryobombs_title"
msgstr "[outline_color=#0000ff]Ability: Cryobombs[/outline_color]"

msgid "cryobombs_description"
msgstr "[outline_size=10]Upon encounter:[/outline_size] Summon [outline_size=10][2][/outline_size] cryobombs. They have [outline_size=10]x[0.2xLEVEL][/outline_size] damage, store taken damage, and explode dealing [outline_size=10]+[STOREDDAMAGE][/outline_size] damage [outline_size=10][3][/outline_size] seconds after getting hit."

msgid "glacial_rail_title"
msgstr "[outline_color=#0000ff]Mastery: Glacial Rail[/outline_color]"

msgid "glacial_rail_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][3][/outline_size] [outline_color=#0000ff]Blue Needle[/outline_color][br]Projectiles gain [outline_size=10]x[1.5][/outline_size] speed."

msgid "snowball_ii_title"
msgstr "[outline_color=#0000ff]Mastery: Snowball II[/outline_color]"

msgid "snowball_ii_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#0000ff]Snowball[/outline_color][br]Projectiles gain [outline_size=10]x[0.25xLEVEL][/outline_size] bonus crit chance and status applied per second."

msgid "snap_freeze_title"
msgstr "[outline_color=#0000ff]Mastery: Snap Freeze[/outline_color]"

msgid "snap_freeze_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#0000ff]Frostbite[/outline_color][br][outline_size=10]Upon dealing damage to an undamaged entity:[/outline_size] Apply freeze for [outline_size=10][0.15xLEVEL][/outline_size] seconds."

msgid "shard_blast_title"
msgstr "[outline_color=#0000ff]Polymastery: Shard Blast[/outline_color]"

msgid "shard_blast_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][3][/outline_size] [outline_color=#0000ff]Shatter[/outline_color] and level [outline_size=10][3][/outline_size] [outline_color=#0000ff]Cryobombs[/outline_color][br]Cryobombs realease [outline_size=10][12][/outline_size] [outline_color=#0000ff]Shatter[/outline_color] projectiles with [outline_size=10]x[2][/outline_size] damage upon exploding."

msgid "chill_title"
msgstr "[outline_color=#0000ff]Notes: Chill[/outline_color]"

msgid "chill_description"
msgstr "Reduces speed to [outline_size=10]x[0.5][/outline_size].[br]Reapplying uses the longer duration value."

msgid "freeze_title"
msgstr "[outline_color=#0000ff]Notes: Freeze[/outline_color]"

msgid "freeze_description"
msgstr "Reduces speed to [outline_size=10]x[0][/outline_size].[br]Reapplying uses the longer duration value."

# lightning
msgid "lightning_title"
msgstr "[outline_color=#a87e00]Lightning[/outline_color]"

msgid "lightning_description"
msgstr "i love electricity."

msgid "chain_bolt_title"
msgstr "[outline_color=#a87e00]Ability: Chain Bolt[/outline_color]"

msgid "chain_bolt_description"
msgstr "Gain [outline_size=10]+[5xLEVEL]%[/outline_size] crit chance.[br][outline_size=10]Upon crit:[/outline_size] Damage chains to the closest enemy up to [outline_size=10][2][/outline_size] times."

msgid "mortal_shock_title"
msgstr "[outline_color=#a87e00]Ability: Mortal Shock[/outline_color]"

msgid "mortal_shock_description"
msgstr "Gain [outline_size=10]+[20xLEVEL]%[/outline_size] crit chance against undamaged enemies and [outline_size=10]+[2xLEVEL][/outline_size] damage on crit."

msgid "ball_lightning_title"
msgstr "[outline_color=#a87e00]Ability: Ball Lightning[/outline_color]"

msgid "ball_lightning_description"
msgstr "[outline_size=10]Upon encounter:[/outline_size] Manifest [outline_size=10][2][/outline_size] ball lightnings orbiting around you. They have [outline_size=10]x[0.5xLEVEL][/outline_size] damage and [outline_size=10]x[2]%[/outline_size] crit chance."

msgid "electromagnetism_title"
msgstr "[outline_color=#a87e00]Ability: Electromagnetism[/outline_color]"

msgid "electromagnetism_description"
msgstr "Projectiles home into enemies within [outline_size=10][50+50xLEVEL]px[/outline_size]."

msgid "thunderstep_title"
msgstr "[outline_color=#a87e00]Ability: Thunderstep[/outline_color]"

msgid "thunderstep_description"
msgstr "You gain [outline_size=10]+[40xLEVEL][/outline_size] move speed and [outline_size=10]+[0.2xLEVEL][/outline_size] seconds of immunity when hit."

msgid "storm_weave_title"
msgstr "[outline_color=#a87e00]Mastery: Storm Weave[/outline_color]"

msgid "storm_weave_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#a87e00]Chain Bolt[/outline_color][br][outline_color=#a87e00]Chain Bolt[/outline_color] can chain [outline_size=10]+[1][/outline_size] time."

msgid "shocking_grasp_title"
msgstr "[outline_color=#a87e00]Mastery: Shocking Grasp[/outline_color]"

msgid "shocking_grasp_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#a87e00]Mortal Shock[/outline_color][br][outline_color=#a87e00]Mortal Shock's[/outline_color] crit chance bonus persists for [outline_size=10][2.5][/outline_size] seconds."

msgid "close_orbit_title"
msgstr "[outline_color=#a87e00]Mastery: Close Orbit[/outline_color]"

msgid "close_orbit_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#a87e00]Ball Lightning[/outline_color][br]Ball lightnings gain [outline_size=10]x[0.6][/outline_size] orbit distance and [outline_size=10]x[2][/outline_size] orbit speed."

msgid "repulsar_title"
msgstr "[outline_color=#a87e00]Mastery: Repulsar[/outline_color]"

msgid "repulsar_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#a87e00]Electromagnetism[/outline_color][br]You divert nearby enemy projectiles."

msgid "arc_shield_title"
msgstr "[outline_color=#a87e00]Mastery: Arc Shield[/outline_color]"

msgid "arc_shield_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#a87e00]Thunderstep[/outline_color][br]You have [outline_size=10][20]%[/outline_size] chance to take [outline_size=10]x[0][/outline_size]."

msgid "crit_title"
msgstr "[outline_color=#a87e00]Notes: Crit[/outline_color]"

msgid "crit_description"
msgstr "[outline_size=10]When dealing damage:[/outline_size] roll for crits.[br]Gain [outline_size=10]x[1+CRITCOUNT][/outline_size] damage and [outline_size=10]+[1][/outline_size] effect chance reroll on crit.[br]Crit chance over [outline_size=10][100]%[/outline_size] is converted into extra crits."

