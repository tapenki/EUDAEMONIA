# red
msgid "red_magic_title"
msgstr "[outline_color=#ff0000]Pyromancy[/outline_color]"

msgid "red_magic_description"
msgstr "Set people on fire yay !!!"

msgid "ignition_title"
msgstr "[outline_color=#ff0000]Ch.1: Ignition[/outline_color]"

msgid "ignition_description"
msgstr "[outline_size=10]Upon dealing damage:[/outline_size] Apply [outline_size=10]+[3xLEVEL][/outline_size] burn."

msgid "firespawning_title"
msgstr "[outline_color=#ff0000]Ch.2: Firespawning[/outline_color]"

msgid "firespawning_description"
msgstr "[outline_size=10]Upon day start:[/outline_size] Manifest [outline_size=10][4][/outline_size] flaming skulls. They have [outline_size=10]+[35xLEVEL][/outline_size] health, [outline_size=10]x[LEVEL][/outline_size] damage, and apply [outline_size=10]+[10xLEVEL][/outline_size] burn to nearby foes when killed."

msgid "fan_the_flames_title"
msgstr "[outline_color=#ff0000]Ch.3: Fan the Flames[/outline_color]"

msgid "fan_the_flames_description"
msgstr "[outline_size=10]Upon applying burn:[/outline_size] That burn gains [outline_size=10]x[0.1xLEVEL][/outline_size] damage."

msgid "scorched_earth_title"
msgstr "[outline_color=#ff0000]Ch.4: Scorched Earth[/outline_color]"

msgid "scorched_earth_description"
msgstr "[outline_size=10]Upon projectile or summon expiration:[/outline_size] It leaves a pool of fire that deals [outline_size=10]x[0.25xLEVEL][/outline_size] damage."

msgid "from_ashes_title"
msgstr "[outline_color=#ff0000]Ch.5: From Ashes[/outline_color]"

msgid "from_ashes_description"
msgstr "[outline_size=10]Once a day, upon imminent demise:[/outline_size] Escape your fate and recover [outline_size=10]+[10xLEVEL][/outline_size] health per second over [outline_size=10][5][/outline_size] seconds."

msgid "conflagration_title"
msgstr "[outline_color=#ff0000]Mastery: Conflagration[/outline_color]"

msgid "conflagration_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#ff0000]Fan the Flames[/outline_color][br][outline_size=10]Upon a burning creature dying:[/outline_size] Spread its burn to the nearest foe."

msgid "burn_title"
msgstr "[outline_color=#ff0000]Notes: Burn[/outline_color]"

msgid "burn_description"
msgstr "Deals [outline_size=10][STACKS][/outline_size] damage every second. Expires after dealing damage [outline_size=10][5][/outline_size] times. Reapplying burn resets expiration."

# blue
msgid "blue_magic_title"
msgstr "[outline_color=#0000ff]Cryomancy[/outline_color]"

msgid "blue_magic_description"
msgstr "Set people on ice yay !!!"

msgid "blue_needle_title"
msgstr "[outline_color=#0000ff]Ch.1: Blue Needle[/outline_color]"

msgid "blue_needle_description"
msgstr "Your projectiles gain [outline_size=10]+[LEVEL][/outline_size] pierce."

msgid "snowball_title"
msgstr "[outline_color=#0000ff]Ch.2: Snowball[/outline_color]"

msgid "snowball_description"
msgstr "Your projectiles gain [outline_size=10]x[0.25xLEVEL][/outline_size] damage and size per second."

msgid "shatter_title"
msgstr "[outline_color=#0000ff]Ch.3: Shatter[/outline_color]"

msgid "shatter_description"
msgstr "[outline_size=10]Upon a creature dying:[/outline_size] It releases [outline_size=10][3][/outline_size] bullets with [outline_size=10]x[LEVEL][/outline_size] damage. Summoned creatures only release [outline_size=10][1][/outline_size] bullet."

msgid "frostbite_title"
msgstr "[outline_color=#0000ff]Ch.4: Frostbite[/outline_color]"

msgid "frostbite_description"
msgstr "[outline_size=10]Upon dealing damage:[/outline_size] Apply a [outline_size=10]x[0.8][/outline_size] speed chill for [outline_size=10]+[0.75xLEVEL][/outline_size] seconds."

msgid "ice_bombs_title"
msgstr "[outline_color=#0000ff]Ch.5: Ice Bombs[/outline_color]"

msgid "ice_bombs_description"
msgstr "[outline_size=10]Upon day start:[/outline_size] Manifest [outline_size=10][2][/outline_size] ice bombs. They have [outline_size=10]x[0.25xLEVEL][/outline_size] damage, store taken damage, and explode dealing [outline_size=10]+[STOREDDAMAGE][/outline_size] damage [outline_size=10][3][/outline_size] seconds after getting hit."

msgid "shard_blast_title"
msgstr "[outline_color=#0000ff]Mastery: Shard Blast[/outline_color]"

msgid "shard_blast_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][3][/outline_size] [outline_color=#0000ff]Shatter[/outline_color] and Level [outline_size=10][3][/outline_size] [outline_color=#0000ff]Ice Bombs[/outline_color][br]Ice bombs realease [outline_size=10][12][/outline_size] [outline_color=#0000ff]Shatter[/outline_color] bullets upon exploding."

# black
msgid "black_magic_title"
msgstr "[outline_color=#ff00ff]Witchcraft[/outline_color]"

msgid "black_magic_description"
msgstr "Which craft??"

msgid "primal_casting_title"
msgstr "[outline_color=#ff00ff]Ch.1: Primal Casting[/outline_color]"

msgid "primal_casting_description"
msgstr "Gain [outline_size=10]+[0.04xMAXHPxLEVEL][/outline_size] damage."

msgid "trail_of_thorns_title"
msgstr "[outline_color=#ff00ff]Ch.2: Trail of Thorns[/outline_color]"

msgid "trail_of_thorns_description"
msgstr "[outline_size=10]Every [75]px moved:[/outline_size] Manifest a thorn that deals [outline_size=10]x[LEVEL][/outline_size] damage."

msgid "quill_spray_title"
msgstr "[outline_color=#ff00ff]Ch.3: Quill Spray[/outline_color]"

msgid "quill_spray_description"
msgstr "[outline_size=10]Upon taking damage:[/outline_size] Deal [outline_size=10]x[2xLEVEL][/outline_size] damage to nearby foes."

msgid "wrought_flesh_title"
msgstr "[outline_color=#ff00ff]Ch.4: Wrought Flesh[/outline_color]"

msgid "wrought_flesh_description"
msgstr "Gain [outline_size=10]+[25xLEVEL][/outline_size] max health. Recover [outline_size=10]+[LEVEL][/outline_size] health per second."

msgid "animated_clay_title"
msgstr "[outline_color=#ff00ff]Ch.5: Animated Clay[/outline_color]"

msgid "animated_clay_description"
msgstr "[outline_size=10]Upon day start:[/outline_size] Manifest a clay effigy. It has [outline_size=10]+[100xLEVEL][/outline_size] health, [outline_size=10]x[0.5+0.5xLEVEL][/outline_size] damage, most of your abilities, and your health bonuses."

msgid "multimold_title"
msgstr "[outline_color=#ff00ff]Mastery: Multimold[/outline_color]"

msgid "multimold_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#ff00ff]Animated Clay[/outline_color][br]Manifest [outline_size=10]+[1][/outline_size] clay effigy. Your clay effigies have [outline_size=10]x[0.5][/outline_size] health."

# white
msgid "white_magic_title"
msgstr "[outline_color=#c19100]Electromancy[/outline_color]"

msgid "white_magic_description"
msgstr "i love electricity."

msgid "chain_bolt_title"
msgstr "[outline_color=#c19100]Ch.1: Chain Bolt[/outline_color]"

msgid "chain_bolt_description"
msgstr "Gain [outline_size=10]+[10xLEVEL]%[/outline_size] crit chance. Critical damage chains to the closest foe up to [outline_size=10][2][/outline_size] times."

msgid "mortal_shock_title"
msgstr "[outline_color=#c19100]Ch.2: Mortal Shock[/outline_color]"

msgid "mortal_shock_description"
msgstr "Gain [outline_size=10]+[25xLEVEL]%[/outline_size] crit chance against undamaged foes and [outline_size=10]+[5xLEVEL][/outline_size] damage on crit."

msgid "electromagnetism_title"
msgstr "[outline_color=#c19100]Ch.3: Electromagnetism[/outline_color]"

msgid "electromagnetism_description"
msgstr "Your projectiles home into enemies within [outline_size=10][50+50xLEVEL]px[/outline_size]."

msgid "ball_lightning_title"
msgstr "[outline_color=#c19100]Ch.4: Ball Lightning[/outline_color]"

msgid "ball_lightning_description"
msgstr "[outline_size=10]Upon day start:[/outline_size] Manifest [outline_size=10][2][/outline_size] ball lightnings orbiting around you. They have [outline_size=10]x[LEVEL][/outline_size] damage and [outline_size=10]x[2]%[/outline_size] crit chance."

msgid "thunderstep_title"
msgstr "[outline_color=#c19100]Ch.5: Thunderstep[/outline_color]"

msgid "thunderstep_description"
msgstr "Gain [outline_size=10]+[50xLEVEL][/outline_size] move speed and [outline_size=10]+[0.2xLEVEL][/outline_size] seconds of immunity when hit."

msgid "dynamo_title"
msgstr "[outline_color=#c19100]Mastery: Dynamo[/outline_color]"

msgid "dynamo_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#c19100]Ball Lightning[/outline_color][br]Your ball lightnings gain [outline_size=10]x[0.25][/outline_size] speed per second up to [outline_size=10]x[1][/outline_size] bonus speed. [outline_size=10]Upon attacking:[/outline_size] Reset the speed bonus."

msgid "crit_title"
msgstr "[outline_color=#c19100]Notes: Crit[/outline_color]"

msgid "crit_description"
msgstr "When dealing damage, roll for crits. Damage is increased by [outline_size=10]x[1+CRITCOUNT][/outline_size]. Each [outline_size=10][100]%[/outline_size] is a guaranteed crit and the rest is rolled as usual."

