msgid "life_title"
msgstr "[outline_color=#0f9936]Life[/outline_color]"

msgid "life_description"
msgstr "I'm alive."

msgid "primal_casting_title"
msgstr "[outline_color=#0f9936]Ability: Primal Casting[/outline_color]"

msgid "primal_casting_description"
msgstr "Gain [outline_size=10]+[0.02xMAXHPxLEVEL][/outline_size] damage."

msgid "wrought_flesh_title"
msgstr "[outline_color=#0f9936]Ability: Wrought Flesh[/outline_color]"

msgid "wrought_flesh_description"
msgstr "You gain [outline_size=10]+[25xLEVEL][/outline_size] max health and recover [outline_size=10]+[LEVEL][/outline_size] health per second."

msgid "trail_of_thorns_title"
msgstr "[outline_color=#0f9936]Ability: Trail of Thorns[/outline_color]"

msgid "trail_of_thorns_description"
msgstr "[outline_size=10]Every [75]px you move:[/outline_size] Manifest a thorn that deals [outline_size=10]x[0.5xLEVEL][/outline_size] damage."

msgid "quill_spray_title"
msgstr "[outline_color=#0f9936]Ability: Quill Spray[/outline_color]"

msgid "quill_spray_description"
msgstr "[outline_size=10]Upon taking damage:[/outline_size] You deal [outline_size=10]x[LEVEL][/outline_size] damage to nearby enemies."

msgid "animated_clay_title"
msgstr "[outline_color=#0f9936]Parability: Animated Clay[/outline_color]"

msgid "animated_clay_description"
msgstr "[outline_size=10]Upon encounter:[/outline_size] Summon a clay effigy. It has [outline_size=10]+[60xLEVEL][/outline_size] health, [outline_size=10]x[0.75+0.25xLEVEL][/outline_size] damage, and your abilities."

msgid "vitalism_title"
msgstr "[outline_color=#0f9936]Mastery: Vitalism[/outline_color]"

msgid "vitalism_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#0f9936]Wrought Flesh[/outline_color][br]You gain [outline_size=10]x[2][/outline_size] health recovery."

msgid "pain_walk_title"
msgstr "[outline_color=#0f9936]Mastery: Pain Walk[/outline_color]"

msgid "pain_walk_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#0f9936]Trail of Thorns[/outline_color][br][outline_size=10]Every [75]px you move:[/outline_size] Manifest [outline_size=10]+[1][/outline_size] thorn. [outline_size=10]Every other activation:[/outline_size] Take [outline_size=10][2][/outline_size] pure damage."

msgid "pressurized_quills_title"
msgstr "[outline_color=#0f9936]Mastery: Pressurized Quills[/outline_color]"

msgid "pressurized_quills_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#0f9936]Quill Spray[/outline_color][br][outline_color=#0f9936]Quill Spray[/outline_color] gains [outline_size=10]x[0.2][/outline_size] bonus damage and size per second for up to [outline_size=10][5][/outline_size] seconds resetting upon activation."

msgid "multimold_title"
msgstr "[outline_color=#0f9936]Mastery: Multimold[/outline_color]"

msgid "multimold_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][3][/outline_size] [outline_color=#0f9936]Animated Clay[/outline_color][br][outline_size=10]Upon encounter:[/outline_size] Summon [outline_size=10]+[1][/outline_size] clay effigy. Clay effigies have [outline_size=10]x[0.5][/outline_size] health."

# death
msgid "death_title"
msgstr "[outline_color=#cc00cc]Death[/outline_color]"

msgid "death_description"
msgstr "I'm dead."

msgid "army_of_the_dead_title"
msgstr "[outline_color=#cc00cc]Ability: Army of the Dead[/outline_color]"

msgid "army_of_the_dead_description"
msgstr "[outline_size=10]Upon slaying a non summoned enemy:[/outline_size] reconstitute it as an ally with [outline_size=10]x[0.4+0.4xLEVEL][/outline_size] of its base health and damage."

msgid "fated_end_title"
msgstr "[outline_color=#cc00cc]Ability: Fated End[/outline_color]"

msgid "fated_end_description"
msgstr "[outline_size=10]Upon dealing damage to an undamaged entity:[/outline_size] Apply [outline_size=10][4xLEVEL][/outline_size] doom."

msgid "hunger_title"
msgstr "[outline_color=#cc00cc]Ability: Hunger[/outline_color]"

msgid "hunger_description"
msgstr "[outline_size=10]Upon an entity dying:[/outline_size] Summons gain a [outline_size=10]x[1.5][/outline_size] speed haste for [outline_size=10]+[0.5xLEVEL][/outline_size] seconds and recover [outline_size=10]+[15xLEVEL][/outline_size] health."

msgid "bone_shield_title"
msgstr "[outline_color=#cc00cc]Ability: Bone Shield[/outline_color]"

msgid "bone_shield_description"
msgstr "[outline_size=10]Upon encounter:[/outline_size] Summon a bone shield. It has [outline_size=10]+[60xLEVEL][/outline_size] health and [outline_size=10]x[LEVEL][/outline_size] damage."

msgid "dark_harvest_title"
msgstr "[outline_color=#cc00cc]Ability: Dark Harvest[/outline_color]"

msgid "dark_harvest_description"
msgstr "[outline_size=10]Upon pressing {keybind}:[/outline_size] A summoned ally explodes dealing [outline_size=10]+[0.2xALLYMAXHP]x[LEVEL][/outline_size] damage."

msgid "swift_fate_title"
msgstr "[outline_color=#cc00cc]Dark Mastery: Swift Fate[/outline_color]"

msgid "swift_fate_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#cc00cc]Fated End[/outline_color][br]Doom you apply has [outline_size=10]x[0.5][/outline_size] duration and [outline_size=10]x[1.5][/outline_size] damage."

msgid "starvation_title"
msgstr "[outline_color=#cc00cc]Dark Mastery: Starvation[/outline_color]"

msgid "starvation_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#cc00cc]Hunger[/outline_color][br][outline_size=10]Upon an entity dying:[/outline_size] Summons gain [outline_size=10]x[0.04xLEVEL][/outline_size] bonus damage."

msgid "osteophalanx_title"
