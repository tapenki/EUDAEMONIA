msgid "wrought_flesh_title"
msgstr "[outline_color=#0f9936]Ch.2: Wrought Flesh[/outline_color]"

msgid "wrought_flesh_description"
msgstr "You gain [outline_size=10]+[25xLEVEL][/outline_size] max health and recover [outline_size=10]+[LEVEL][/outline_size] health per second."

msgid "trail_of_thorns_title"
msgstr "[outline_color=#0f9936]Ch.3: Trail of Thorns[/outline_color]"

msgid "trail_of_thorns_description"
msgstr "[outline_size=10]Every [75]px you move:[/outline_size] Manifest a thorn that deals [outline_size=10]x[LEVEL][/outline_size] damage."

msgid "quill_spray_title"
msgstr "[outline_color=#0f9936]Ch.4: Quill Spray[/outline_color]"

msgid "quill_spray_description"
msgstr "[outline_size=10]Upon your taking damage:[/outline_size] Deal [outline_size=10]x[2xLEVEL][/outline_size] damage to nearby foes."

msgid "animated_clay_title"
msgstr "[outline_color=#0f9936]Ch.5: Animated Clay[/outline_color]"

msgid "animated_clay_description"
msgstr "[outline_size=10]Upon day start:[/outline_size] Summon a clay effigy. It has [outline_size=10]+[60xLEVEL][/outline_size] health, [outline_size=10]x[0.75+0.25xLEVEL][/outline_size] damage, and your abilities."

msgid "pain_walk_title"
msgstr "[outline_color=#0f9936]Polymastery: Pain Walk[/outline_color]"

msgid "pain_walk_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][3][/outline_size] [outline_color=#0f9936]Wrought Flesh[/outline_color] and level [outline_size=10][3][/outline_size] [outline_color=#0f9936]Trail of Thorns[/outline_color][br][outline_size=10]Every [75]px you move:[/outline_size] Manifest [outline_size=10]+[1][/outline_size] thorn. [outline_size=10]Every other activation:[/outline_size] Take [outline_size=10][2][/outline_size] piercing damage."

msgid "pressurized_quills_title"
msgstr "[outline_color=#0f9936]Mastery: Pressurized Quills[/outline_color]"

msgid "pressurized_quills_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#0f9936]Quill Spray[/outline_color][br][outline_color=#0f9936]Quill Spray[/outline_color] gains [outline_size=10]x[0.2][/outline_size] bonus damage and size per second for up to [outline_size=10][5][/outline_size] seconds resetting upon activation."

msgid "multimold_title"
msgstr "[outline_color=#0f9936]Mastery: Multimold[/outline_color]"

msgid "multimold_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#0f9936]Animated Clay[/outline_color][br][outline_size=10]Upon day start:[/outline_size] Summon [outline_size=10]+[1][/outline_size] clay effigy. Clay effigies have [outline_size=10]x[0.5][/outline_size] health."

# necromancy

msgid "necromancy_title"
msgstr "[outline_color=#cc00cc]Necromancy[/outline_color]"

msgid "necromancy_description"
msgstr "I'm dead."

msgid "army_of_the_dead_title"
msgstr "[outline_color=#cc00cc]Ch.1: Army of the Dead[/outline_color]"

msgid "army_of_the_dead_description"
msgstr "[outline_size=10]Upon slaying a non summoned foe:[/outline_size] reconstitute it as an ally with [outline_size=10]x[0.5+0.5xLEVEL][/outline_size] health and damage."

msgid "death_touch_title"
msgstr "[outline_color=#cc00cc]Ch.2: Death Touch[/outline_color]"

msgid "death_touch_description"
msgstr "Gain a [outline_size=10]+[LEVEL]%[/outline_size] chance to deal [outline_size=10]x[100][/outline_size] damage."

msgid "hunger_title"
msgstr "[outline_color=#cc00cc]Ch.3: Hunger[/outline_color]"

msgid "hunger_description"
msgstr "[outline_size=10]Upon a creature dying:[/outline_size] Summons gain a [outline_size=10]x[1.5][/outline_size] speed haste for [outline_size=10]+[0.5xLEVEL][/outline_size] seconds and recover [outline_size=10]+[15xLEVEL][/outline_size] health."

msgid "bone_shield_title"
msgstr "[outline_color=#cc00cc]Ch.4: Bone Shield[/outline_color]"

msgid "bone_shield_description"
msgstr "[outline_size=10]Upon day start:[/outline_size] Summon a bone shield. It has [outline_size=10]+[60xLEVEL][/outline_size] health and [outline_size=10]x[LEVEL][/outline_size] damage."

msgid "dark_harvest_title"
msgstr "[outline_color=#cc00cc]Ch.5: Dark Harvest[/outline_color]"

msgid "dark_harvest_description"
msgstr "[outline_size=10]Upon pressing {keybind}:[/outline_size] A summoned ally explodes dealing [outline_size=10]+[0.25xALLYMAXHP]x[LEVEL][/outline_size] damage."

msgid "ante_mortem_title"
msgstr "[outline_color=#cc00cc]Dark Mastery: Ante Mortem[/outline_color]"

msgid "ante_mortem_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#cc00cc]Death Touch[/outline_color][br][outline_size=10]Upon [/outline_size][outline_color=#cc00cc]Death Touch[/outline_color][outline_size=10] activation:[/outline_size] Activate death effects for the affected creature."

msgid "starvation_title"
msgstr "[outline_color=#cc00cc]Dark Mastery: Starvation[/outline_color]"

msgid "starvation_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#cc00cc]Hunger[/outline_color][br][outline_size=10]Upon a creature dying:[/outline_size] Summons gain [outline_size=10]x[0.04xHUNGERLEVEL][/outline_size] bonus damage."

msgid "bone_spear_title"
msgstr "[outline_color=#cc00cc]Dark Mastery: Bone Spear[/outline_color]"

msgid "bone_spear_description"
msgstr "[outline_size=10]Requires:[/outline_size] Level [outline_size=10][5][/outline_size] [outline_color=#cc00cc]Bone Shield[/outline_color][br][outline_size=10]Upon day start:[/outline_size] Summon [outline_size=10]+[3][/outline_size] bone shields."


